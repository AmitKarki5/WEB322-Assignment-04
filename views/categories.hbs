<div class="container" style="padding-top:60px">
  <div class="row">
    <div class="col-md-12">
      <h2>Categories</h2>
      <hr />
      <div class="table-responsive">
        <table class="table">
          <thead>
            <tr>
              <th>Category ID</th>
              <th>Category Name</th>
            </tr>
          </thead>
          <tbody>
            {{#each categories}}
              <tr>
                <td><a href="/demos?category={{category}}"></a>{{{id}}}</a></td>
                <td><a href="/demos?category={{category}}">{{category}}</a></td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
</div>