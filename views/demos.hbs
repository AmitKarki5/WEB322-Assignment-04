<div class="container" style="padding-top:60px">
  <div class="row">
    <div class="col-md-12">
      <h2>Demos</h2>
      <hr />
      <div class="table-responsive">
        <table class="table">
          <thead>
            <tr>
              <th>ID</th>
              <th>Title</th>
              <th>Post Date</th>
              <th>Category</th>
              <th>Published</th>
            </tr>
          </thead>
          <tbody>
            {{#each products}}
              <tr>
                <td>{{{id}}}</td>
                <td>{{{title}}}</td>
                <td>{{{postDate}}}</td>
                <td><a href="/demos?category={{category}}">{{category}}</a></td>
                <td>{{{published}}}</td>
                <td>{{{hireDate}}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
</div>